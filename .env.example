# Copy this file to `.env` and update the values for your environment.
# Never commit the populated `.env` file to version control.

# Toggle pgvector sink for embeddings
DOCINTEL_VECTOR_DB_ENABLED=1

# PostgreSQL connection string used for the vector store (update with real credentials)
# Example: postgresql://username:password@hostname:5432/docintel
DOCINTEL_VECTOR_DB_DSN=postgresql://USER:PASSWORD@HOST:5432/docintel

# Optional: legacy aliases remain supported by the settings loader
DOCINTEL_ENABLED=1
DOCINTEL_DSN=${DOCINTEL_VECTOR_DB_DSN}
